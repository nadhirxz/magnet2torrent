	"dependencies": {
		"bittorrent-protocol": "^3.4.3",
		"chalk": "^4.1.2",
		"commander": "^8.3.0",
		"ora": "^5.4.1",
		"parse-torrent": "^9.1.4",
		"pkginfo": "^0.4.1",
		"torrent-discovery": "^9.4.7",
		"ut_metadata": "^3.5.2"
	}
}
