{
	"name": "magnet2torrent",
	"version": "1.0.0",
	"description": "Convert magnet links to torrent files",
	"main": "index.js",
	"preferGlobal": true,
	"bin": "./index.js",
	"scripts": {
		"package": "npx pkg package.json --out-path dist",
		"package-linux": "npm run package -- -t node18-linux-x64",
		"package-mac": "npm run package -- -t node18-macos-x64",
		"package-win": "npm run package -- -t node18-win-x64"
	},
	"author": "Nadhir Ferdji",
	"repository": {
		"type": "git",
		"url": "https://github.com/nadhirxz/magnet2torrent.git"
	},
	"license": "MIT",
	"dependencies": {
		"bittorrent-dht": "^10.0.7",
		"bittorrent-protocol": "^3.5.4",
		"chalk": "^4.1.2",
		"commander": "^11.0.0",
		"ora": "^5.4.1",
		"parse-torrent": "^9.1.5",
		"pkginfo": "^0.4.1",
		"ut_metadata": "^3.5.2"
	}
}
